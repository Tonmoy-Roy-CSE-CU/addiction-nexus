adnx:hasMedicationUsage a owl:ObjectProperty ;
    rdfs:domain adnx:MentalHealth ;
    rdfs:range adnx:Medication .

adnx:hasMentalHealth a owl:ObjectProperty ;
    rdfs:domain adnx:Person ;
    rdfs:range adnx:MentalHealth .

adnx:hasObservation a owl:ObjectProperty ;
