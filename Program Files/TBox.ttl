@prefix adnx: <https://purl.archive.org/addiction-nexus#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qb: <http://purl.org/linked-data/cube#> .
@prefix qb4o: <http://purl.org/qb4olap/cubes#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .

adnx:MentalHealthDataSet a qb:DataSet ;
    rdfs:label "Mental Health Data Set"@en ;
    rdfs:comment "QB dataset for mental health analytics" ;
    dct:title "Addiction Nexus Knowledge Graph (ADNXKG) - Version 3.0 - 50,000 Records" ;
    dct:description "A comprehensive knowledge graph comprising 50,000 mental health and lifestyle records, designed for multidimensional OLAP analytics and adhering to FAIR principles (Findable, Accessible, Interoperable, Reusable). This dataset integrates a TBox (ontological structure) and ABox (instance data) in RDF/TTL format, utilizing QB4OLAP and OWL vocabularies for semantic interoperability. It includes dimensions such as age, gender, country, occupation, severity, and stress levels, with measures like physical activity hours, sleep hours, social media usage, and work hours. The dataset is licensed under Creative Commons Attribution 4.0 International (CC-BY 4.0) and is accessible via a persistent DOI. Metadata includes title, description, creator (Tonmoy Roy), publisher (University of Chittagong), and access URL, ensuring discoverability and reusability. This resource supports addiction research, enabling SPARQL-driven analysis and integration with Linked Open Data (LOD) sources like Wikidata. Uploaded on August 24, 2025, for open-access distribution and further research engagement." ;
    dcat:accessURL <https://zenodo.org/records/[new-record-id]/files/tbox_v3.ttl> ;
    dct:license <http://creativecommons.org/licenses/by/4.0/> ;
    dct:creator "Tonmoy Roy" ;
    dct:publisher "University of Chittagong" .

adnx:AgeLevel a qb4o:Level ;
    rdfs:label "Individual Age Level"@en ;
    qb4o:inHierarchy adnx:AgeHierarchy ;
    qb4o:parentLevel adnx:AgeGroupLevel .

adnx:CountryLevel a qb4o:Level ;
    rdfs:label "Country Level"@en ;
    qb4o:inHierarchy adnx:CountryHierarchy .

adnx:GenderLevel a qb4o:Level ;
    rdfs:label "Gender Level"@en ;
    qb4o:inHierarchy adnx:GenderHierarchy .

adnx:MentalHealthCube a qb4o:Cube ;
    rdfs:label "Mental Health Analysis Cube"@en ;
    qb4o:dimension adnx:ageDimension,
        adnx:countryDimension,
        adnx:genderDimension,
        adnx:occupationDimension,
        adnx:severityDimension,
        adnx:stressLevelDimension ;
    qb4o:measure adnx:ageMeasure,
        adnx:physicalActivityMeasure,
        adnx:sleepHoursMeasure,
        adnx:socialMediaMeasure,
        adnx:workHoursMeasure ;
    rdfs:comment "QB4OLAP cube for mental health analytics" .

adnx:OccupationLevel a qb4o:Level ;
    rdfs:label "Occupation Level"@en ;
    qb4o:inHierarchy adnx:OccupationHierarchy .

adnx:SeverityLevel a qb4o:Level ;
    rdfs:label "Severity Level"@en ;
    qb4o:inHierarchy adnx:SeverityHierarchy .

adnx:StressLevelBase a qb4o:Level ;
    rdfs:label "Stress Level"@en ;
    qb4o:inHierarchy adnx:StressHierarchy .

adnx:avgFunction a qb4o:AggregateFunction ;
    rdfs:label "Average Function"@en .

adnx:countFunction a qb4o:AggregateFunction ;
    rdfs:label "Count Function"@en .

